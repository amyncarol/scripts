   "source": [
    "errors = zeros(20,1);\n",
    "for m = 5:5:100;\n",
    "    b = randn(m, 1);\n",
    "    A = zeros(m);\n",
    "    for i=1:m\n",
    "        A(i,i) = 1;\n",
    "        A(i, m) = 1;\n",
    "        for j = 1:(i-1)\n",
    "            A(i, j) = -1;\n",
    "        end\n",
    "    end\n",
    "    x = A\\b;\n",
    "    error(m/5,1) = norm(A*x - b)\n",
    "end"
   ]
