   "source": [
    "nsample = 100000\n",
    "x <- matrix(rpois(n=20*nsample, lambda=1), nsample)\n",
    "reject = apply(x, 1, function(x) abs((mean(x)-1)/sqrt(mean(x)/20))>-qnorm(0.025))\n",
    "sum(reject)/nsample"
